{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug with Places API",
      "request": "launch",
      "type": "dart",
      "toolArgs": [
        "--dart-define-from-file=env.json"
      ]
    },
    {
      "name": "Debug (Manual API Key)",
      "request": "launch", 
      "type": "dart",
      "toolArgs": [
        "--dart-define=GOOGLE_PLACES_API_KEY=YOUR_REAL_KEY_HERE"
      ]
    },
    {
      "name": "Release with Places API",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "toolArgs": [
        "--dart-define-from-file=env.json"
      ]
    }
  ]
}